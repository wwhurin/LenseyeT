<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kr.hs.emirim.wwhurin.lenseye5.SecondFragment"

    android:background="@drawable/back_w">

    <!-- TODO: Update blank fragment layout -->


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="21dp"
        android:background="@drawable/allmenu"
        android:weightSum="1">

        <Button
            android:id="@+id/colorbtn"
            android:layout_width="129dp"
            android:layout_height="102dp"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_weight="0.71"
            android:background="#00000000"
            android:textAlignment="center"
            android:textColor="#516f9e"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/noseebtn"
            android:layout_width="129dp"
            android:layout_height="102dp"
            android:layout_alignParentTop="true"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_toEndOf="@+id/colorbtn"
            android:layout_toRightOf="@+id/colorbtn"
            android:layout_weight="0.71"
            android:background="#00000000"
            android:textAlignment="center"
            android:textColor="#516f9e" />

        <Button
            android:id="@+id/onedaybtn"
            android:layout_width="129dp"
            android:layout_height="102dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_weight="0.71"
            android:background="#00000000"
            android:textAlignment="center"
            android:textColor="#516f9e" />

        <Button
            android:id="@+id/twoweekbtn"
            android:layout_width="129dp"
            android:layout_height="102dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/colorbtn"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_weight="0.71"
            android:background="#00000000"
            android:textAlignment="center"
            android:textColor="#516f9e" />

        <Button
            android:id="@+id/monthbtn"
            android:layout_width="129dp"
            android:layout_height="102dp"
            android:layout_alignParentBottom="true"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_toLeftOf="@+id/onedaybtn"
            android:layout_toStartOf="@+id/onedaybtn"
            android:layout_weight="0.71"
            android:background="#00000000"
            android:textAlignment="center"
            android:textColor="#516f9e" />

        <Button
            android:id="@+id/ALL"
            android:layout_width="129dp"
            android:layout_height="102dp"
            android:layout_alignParentBottom="true"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_toEndOf="@+id/noseebtn"
            android:layout_toRightOf="@+id/noseebtn"
            android:layout_weight="0.71"
            android:background="#00000000"
            android:textAlignment="center"
            android:textColor="#516f9e" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="컬러 렌즈"
            android:textColor="#516f9e"
            android:textAlignment="center"
            android:layout_alignBottom="@+id/colorbtn"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/noseebtn" android:gravity="center"
            android:layout_alignParentTop="true" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="투명 렌즈"
            android:textColor="#516f9e"
            android:textAlignment="center" android:gravity="center"
            android:layout_alignBottom="@+id/noseebtn"
            android:layout_toLeftOf="@+id/onedaybtn"
            android:layout_alignLeft="@+id/monthbtn"
            android:layout_alignStart="@+id/monthbtn"
            android:layout_alignParentTop="true" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/twoweekbtn"
            android:layout_alignEnd="@+id/twoweekbtn"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignRight="@+id/twoweekbtn"
            android:text="2주 렌즈"
            android:textColor="#516f9e"
            android:textAlignment="center" android:gravity="center"
            android:layout_alignTop="@+id/monthbtn" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/textView4"
            android:layout_toRightOf="@+id/noseebtn"
            android:text="원데이 렌즈"
            android:textColor="#516f9e"
            android:textAlignment="center" android:gravity="center"
            android:layout_alignBottom="@+id/noseebtn" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="한달 렌즈"
            android:textColor="#516f9e"
            android:textAlignment="center" android:gravity="center"
            android:layout_alignBottom="@+id/monthbtn"
            android:layout_alignLeft="@+id/monthbtn"
            android:layout_alignStart="@+id/monthbtn"
            android:layout_below="@+id/colorbtn"
            android:layout_toLeftOf="@+id/onedaybtn"
            android:layout_toStartOf="@+id/onedaybtn" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ALL"
            android:layout_alignBottom="@+id/ALL"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignLeft="@+id/ALL"
            android:layout_alignStart="@+id/ALL"
            android:textColor="#516f9e"
            android:textAlignment="center"
            android:gravity="center"
            android:layout_alignTop="@+id/ALL" />

    </RelativeLayout>

</LinearLayout>
